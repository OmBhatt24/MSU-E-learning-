<%-include('partials/header'); -%> <%-include('partials/navbar'); -%>


<% courseArr.forEach((ele)=>{ %>

  <section class="careusel-cse">

    <center><h1><%= ele.domain %></h1></center>
  
  <hr class="style-seven">
  
  <div class="cards">
  
    <% ele.cardlist.forEach((e)=>{ %>

      <div class="card">
        <img src="/images/<%=e.imgurl %>" alt="thumbnail" />
        <h2><%= e.cardTitle %></h2>
        <div class="card-content">
          <p>
           <%= e.desc %>
          </p>
          <h3><%= e.author %></h3>
          <a href="/courses/<%= courseName %>/pcourse/<%= ele.itemid %>/student/<%= e.cardId %>"><button class="btn-1" >Explore</button></a>
        </div>
      </div>
   <% }); %>
    
  </div>
  
  </section>

<% }); %>

<script>
  console.log("Hello");
  document.querySelectorAll(".signUp-button")[0].addEventListener("click", function () {
    document.getElementById("popup").style.display = "flex";
  });

  document.getElementById("close").addEventListener("click", function () {
    document.getElementById("popup").style.display = "none";
    console.log("Hello");
  });

</script>

<%-include('partials/footer'); -%>
